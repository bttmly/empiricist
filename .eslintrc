{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "func-style": [0, "declaration"],
    "handle-callback-err": 2,
    "strict": [2, "global"],
    "curly": 0,
    "strict": [2, "never"],
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": [2, "always"],
    "no-undef": 0,
    "comma-dangle": [2, "always-multiline"]
  },
  "globals": {
    "it": 0,
    "describe": 0
  }
}
